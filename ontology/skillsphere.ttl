@prefix sk: <https://ontology.skillsphere.dev/schema#> .
@prefix schema: <https://schema.org/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix skos: <http://www.w3.org/2004/02/skos/core#> .
@prefix prov: <http://www.w3.org/ns/prov#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/> .

sk:Ontology a owl:Ontology ;
  dcterms:title "SkillSphere Ontology" ;
  dcterms:description "An ontology derived from the SkillSphere proto APIs that captures users, skills, sessions, matching, and recommendation semantics." ;
  dcterms:creator "SkillSphere" ;
  dcterms:source "https://github.com/FACorreiaa/skillsphere-proto" ;
  owl:versionInfo "0.1.0" .

# ---------------------------------------------------------------------------
# Core Classes
# ---------------------------------------------------------------------------

sk:User a owl:Class ;
  rdfs:label "SkillSphere User" ;
  skos:definition "An individual participating in the SkillSphere peer-to-peer skill exchange." ;
  rdfs:subClassOf foaf:Person .

sk:Skill a owl:Class ;
  rdfs:label "Skill" ;
  skos:definition "A discrete ability that can be offered or requested within SkillSphere." ;
  rdfs:subClassOf schema:Intangible , skos:Concept .

sk:SkillAssociation a owl:Class ;
  rdfs:label "Skill Association" ;
  skos:definition "A contextual link between a user and a skill describing proficiency, demand, and offering intent." .

sk:SkillCategoryScheme a skos:ConceptScheme ;
  rdfs:label "Skill Categories" .

sk:ProficiencyLevelScheme a skos:ConceptScheme ;
  rdfs:label "Proficiency Levels" .

sk:SubscriptionTierScheme a skos:ConceptScheme ;
  rdfs:label "Subscription Tiers" .

sk:SessionStatusScheme a skos:ConceptScheme ;
  rdfs:label "Session Status Values" .

sk:MatchingAlgorithmScheme a skos:ConceptScheme ;
  rdfs:label "Matching Algorithms" .

sk:RecommendationTypeScheme a skos:ConceptScheme ;
  rdfs:label "Recommendation Types" .

sk:Availability a owl:Class ;
  rdfs:label "Availability" ;
  skos:definition "A reusable block describing where and when a user is available for exchanges." .

sk:TimeSlot a owl:Class ;
  rdfs:label "Time Slot" ;
  skos:definition "A recurring weekly window for scheduling sessions." .

sk:Session a owl:Class ;
  rdfs:label "Skill Exchange Session" ;
  rdfs:subClassOf schema:Event ;
  skos:definition "A scheduled interaction between two SkillSphere users to exchange skills." .

sk:Match a owl:Class ;
  rdfs:label "Match" ;
  skos:definition "A computed pairing between two users scored by one of the matching algorithms." ;
  rdfs:subClassOf prov:Activity .

sk:SkillMatch a owl:Class ;
  rdfs:label "Skill Match" ;
  skos:definition "Explains how specific skills complement each user in a match." .

sk:Recommendation a owl:Class ;
  rdfs:label "Recommendation" ;
  skos:definition "A personalized suggestion issued to a user for potential partners, skills, or sessions." ;
  rdfs:subClassOf schema:CreativeWork .

sk:SkillEmbedding a owl:Class ;
  rdfs:label "Skill Embedding" ;
  skos:definition "A vector representation of a skill or profile generated by the AI service." ;
  rdfs:subClassOf prov:Entity .

sk:Rating a owl:Class ;
  rdfs:label "Rating" ;
  rdfs:subClassOf schema:Review ;
  skos:definition "A qualitative and quantitative assessment recorded after a session." .

# ---------------------------------------------------------------------------
# Object Properties
# ---------------------------------------------------------------------------

sk:offersSkill a owl:ObjectProperty ;
  rdfs:domain sk:User ;
  rdfs:range sk:Skill ;
  rdfs:subPropertyOf schema:knowsAbout ;
  rdfs:label "offers skill" ;
  skos:definition "Links a user to a skill they are willing to teach." .

sk:wantsSkill a owl:ObjectProperty ;
  rdfs:domain sk:User ;
  rdfs:range sk:Skill ;
  rdfs:subPropertyOf schema:wants ;
  rdfs:label "wants skill" ;
  skos:definition "Links a user to a skill they would like to learn." .

sk:hasSkillAssociation a owl:ObjectProperty ;
  rdfs:domain sk:User ;
  rdfs:range sk:SkillAssociation ;
  rdfs:label "has skill association" ;
  skos:definition "Connects a user to the structured representation of their relationship with a skill." .

sk:associationSkill a owl:ObjectProperty ;
  rdfs:domain sk:SkillAssociation ;
  rdfs:range sk:Skill ;
  rdfs:label "association skill" .

sk:proficiencyLevel a owl:ObjectProperty ;
  rdfs:domain sk:SkillAssociation ;
  rdfs:range sk:ProficiencyLevelConcept ;
  rdfs:label "proficiency level" .

sk:subscriptionTier a owl:ObjectProperty ;
  rdfs:domain sk:User ;
  rdfs:range sk:SubscriptionTierConcept ;
  rdfs:label "subscription tier" .

sk:hasAvailability a owl:ObjectProperty ;
  rdfs:domain sk:User ;
  rdfs:range sk:Availability ;
  rdfs:label "has availability" .

sk:availableDuring a owl:ObjectProperty ;
  rdfs:domain sk:Availability ;
  rdfs:range sk:TimeSlot ;
  rdfs:label "available during" .

sk:sessionStatus a owl:ObjectProperty ;
  rdfs:domain sk:Session ;
  rdfs:range sk:SessionStatusConcept ;
  rdfs:label "session status" .

sk:initiatedBy a owl:ObjectProperty ;
  rdfs:domain sk:Session ;
  rdfs:range sk:User ;
  rdfs:label "initiated by" .

sk:hasParticipant a owl:ObjectProperty ;
  rdfs:domain sk:Session ;
  rdfs:range sk:User ;
  rdfs:label "has participant" ;
  rdfs:subPropertyOf schema:attendee .

sk:focusedOnSkill a owl:ObjectProperty ;
  rdfs:domain sk:Session ;
  rdfs:range sk:Skill ;
  rdfs:label "focused on skill" .

sk:producesRating a owl:ObjectProperty ;
  rdfs:domain sk:Session ;
  rdfs:range sk:Rating ;
  rdfs:label "produces rating" .

sk:ratedEntity a owl:ObjectProperty ;
  rdfs:domain sk:Rating ;
  rdfs:range sk:User ;
  rdfs:label "rated entity" ;
  rdfs:subPropertyOf schema:itemReviewed .

sk:hasMatch a owl:ObjectProperty ;
  rdfs:domain sk:User ;
  rdfs:range sk:Match ;
  rdfs:label "has match" .

sk:matchTarget a owl:ObjectProperty ;
  rdfs:domain sk:Match ;
  rdfs:range sk:User ;
  rdfs:label "match target" .

sk:explainedBy a owl:ObjectProperty ;
  rdfs:domain sk:Match ;
  rdfs:range sk:SkillMatch ;
  rdfs:label "explained by" .

sk:matchingAlgorithm a owl:ObjectProperty ;
  rdfs:domain sk:Match ;
  rdfs:range sk:MatchingAlgorithmConcept ;
  rdfs:label "matching algorithm" .

sk:skillComplement a owl:ObjectProperty ;
  rdfs:domain sk:SkillMatch ;
  rdfs:range sk:Skill ;
  rdfs:label "skill complement" .

sk:recommendationType a owl:ObjectProperty ;
  rdfs:domain sk:Recommendation ;
  rdfs:range sk:RecommendationTypeConcept ;
  rdfs:label "recommendation type" .

sk:recommendsItem a owl:ObjectProperty ;
  rdfs:domain sk:Recommendation ;
  rdfs:range owl:Thing ;
  rdfs:label "recommends item" ;
  rdfs:subPropertyOf schema:suggestedAnswer .

sk:generatedFor a owl:ObjectProperty ;
  rdfs:domain sk:Recommendation ;
  rdfs:range sk:User ;
  rdfs:label "generated for" .

sk:embeddingForSkill a owl:ObjectProperty ;
  rdfs:domain sk:SkillEmbedding ;
  rdfs:range sk:Skill ;
  rdfs:label "embedding for skill" .

sk:embeddingForUser a owl:ObjectProperty ;
  rdfs:domain sk:SkillEmbedding ;
  rdfs:range sk:User ;
  rdfs:label "embedding for user" .

# ---------------------------------------------------------------------------
# Datatype Properties
# ---------------------------------------------------------------------------

sk:proficiencyValue a owl:DatatypeProperty ;
  rdfs:domain sk:SkillAssociation ;
  rdfs:range xsd:integer ;
  rdfs:label "proficiency value" ;
  skos:definition "Exact 1-10 proficiency score captured in UserSkill." .

sk:experienceText a owl:DatatypeProperty ;
  rdfs:domain sk:SkillAssociation ;
  rdfs:range xsd:string ;
  rdfs:label "experience text" .

sk:matchScore a owl:DatatypeProperty ;
  rdfs:domain sk:Match ;
  rdfs:range xsd:float ;
  rdfs:label "match score" .

sk:isMutual a owl:DatatypeProperty ;
  rdfs:domain sk:Match ;
  rdfs:range xsd:boolean ;
  rdfs:label "is mutual" .

sk:similarityScore a owl:DatatypeProperty ;
  rdfs:domain sk:SkillMatch ;
  rdfs:range xsd:float ;
  rdfs:label "similarity score" .

sk:isComplementary a owl:DatatypeProperty ;
  rdfs:domain sk:SkillMatch ;
  rdfs:range xsd:boolean ;
  rdfs:label "is complementary" .

sk:relevanceScore a owl:DatatypeProperty ;
  rdfs:domain sk:Recommendation ;
  rdfs:range xsd:float ;
  rdfs:label "relevance score" .

sk:reasonText a owl:DatatypeProperty ;
  rdfs:domain sk:Recommendation ;
  rdfs:range xsd:string ;
  rdfs:label "reason text" .

sk:scheduledStart a owl:DatatypeProperty ;
  rdfs:domain sk:Session ;
  rdfs:range xsd:dateTime ;
  rdfs:label "scheduled start" ;
  rdfs:subPropertyOf schema:startDate .

sk:scheduledEnd a owl:DatatypeProperty ;
  rdfs:domain sk:Session ;
  rdfs:range xsd:dateTime ;
  rdfs:label "scheduled end" ;
  rdfs:subPropertyOf schema:endDate .

sk:actualStart a owl:DatatypeProperty ;
  rdfs:domain sk:Session ;
  rdfs:range xsd:dateTime ;
  rdfs:label "actual start" .

sk:actualEnd a owl:DatatypeProperty ;
  rdfs:domain sk:Session ;
  rdfs:range xsd:dateTime ;
  rdfs:label "actual end" .

sk:meetingUrl a owl:DatatypeProperty ;
  rdfs:domain sk:Session ;
  rdfs:range xsd:anyURI ;
  rdfs:label "meeting url" ;
  rdfs:subPropertyOf schema:url .

sk:sessionNotes a owl:DatatypeProperty ;
  rdfs:domain sk:Session ;
  rdfs:range xsd:string ;
  rdfs:label "session notes" .

sk:availabilityTimezone a owl:DatatypeProperty ;
  rdfs:domain sk:Availability ;
  rdfs:range xsd:string ;
  rdfs:label "availability timezone" .

sk:dayOfWeek a owl:DatatypeProperty ;
  rdfs:domain sk:TimeSlot ;
  rdfs:range xsd:string ;
  rdfs:label "day of week" .

sk:startTime a owl:DatatypeProperty ;
  rdfs:domain sk:TimeSlot ;
  rdfs:range xsd:string ;
  rdfs:label "start time" .

sk:endTime a owl:DatatypeProperty ;
  rdfs:domain sk:TimeSlot ;
  rdfs:range xsd:string ;
  rdfs:label "end time" .

sk:ratingValue a owl:DatatypeProperty ;
  rdfs:domain sk:Rating ;
  rdfs:range xsd:integer ;
  rdfs:label "rating value" ;
  rdfs:subPropertyOf schema:ratingValue .

sk:ratingComment a owl:DatatypeProperty ;
  rdfs:domain sk:Rating ;
  rdfs:range xsd:string ;
  rdfs:label "rating comment" ;
  rdfs:subPropertyOf schema:reviewBody .

sk:embeddingVector a owl:DatatypeProperty ;
  rdfs:domain sk:SkillEmbedding ;
  rdfs:range rdf:List ;
  rdfs:label "embedding vector" ;
  skos:definition "Ordered floating-point values representing the pgvector column." .

sk:modelVersion a owl:DatatypeProperty ;
  rdfs:domain sk:SkillEmbedding ;
  rdfs:range xsd:string ;
  rdfs:label "model version" .
